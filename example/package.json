{
  "name": "example",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "install-radar": "npm un --no-save react-native-radar ; npm i --no-save --install-links",
    "install-radar-rebuild-dirty": "npm un --no-save react-native-radar ; npm i --no-save --install-links ../ && expo prebuild",
    "install-radar-rebuild": "npm un --no-save react-native-radar ; npm i --no-save --install-links ../ && expo prebuild --clean"
  },
  "dependencies": {
    "expo": "~50.0.11",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-radar": "3.10.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3"
  },
  "private": true
}
